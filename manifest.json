{
	"name": "Saptarangi",
	"description": "Change webpage background to any of the seven colors in a rainbow (Saptarangi).",
	"version": "0.0.1",
	"manifest_version": 2,
	"browser_action": {
		"default_title": "Embrace the Saptarangi!",
		"default_icon": "res/icon16.png"
	},
	"permissions": [
		"activeTab"
	],
	"background" : {
		"scripts": ["background.js"],
		"persistent": false
	}
	"icons": {
		"16": "res/icon16.png",
		"32": "res/icon32.png",
		"48": "res/icon48.png",
		"128": "res/icon128/png"
	}
}
